{
  "name": "my-react-router-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "deploy": "wrangler deploy",
    "dev": "react-router dev",
    "start": "wrangler dev",
    "typegen": "wrangler types",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .; stylelint --cache --cache-location ./node_modules/.cache/stylelint 'app/**/*.css'",
    "typecheck": "react-router typegen && tsc",
    "auth:generate": "npx @better-auth/cli generate --config ./auth.config.ts",
    "db:delete": "wrangler d1 delete rtrr-cf-workers-test",
    "db:create": "wrangler d1 create rtrr-cf-workers-test",
    "db:generate": "drizzle-kit generate --config drizzle-local.config.ts",
    "db:migrate-local": "wrangler d1 migrations apply rtrr-cf-workers-test --local",
    "db:migrate-prod": "wrangler d1 migrations apply rtrr-cf-workers-test --remote",
    "db:drizzle-push-local": "drizzle-kit push --config drizzle-local.config.ts ",
    "db:drizzle-migrate-prod": "drizzle-kit migrate --config drizzle-prod.config.ts",
    "db:drop-all-local": "rm -rf .wrangler/state/v3/d1",
    "db:drop-all-prod": "echo \"ðŸ”¥ Be careful ðŸ”¥\nYou can delete all tables in the production environment database by executing the following command.\n> wrangler d1 execute rtrr-cf-workers-test --remote --file='./db/seed/drop-all-table.sql'\"",
    "db:view-local": "drizzle-kit studio --config drizzle-local.config.ts",
    "db:view-prod": "drizzle-kit studio --config drizzle-prod.config.ts"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250321.0",
    "@react-router/cloudflare": "^7.4.0",
    "@react-router/fs-routes": "^7.4.0",
    "@react-router/node": "^7.4.0",
    "@react-router/serve": "^7.4.0",
    "better-auth": "^1.2.4",
    "drizzle-orm": "^0.41.0",
    "isbot": "^5.1.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.4.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.23.0",
    "@react-router/dev": "^7.4.0",
    "@tailwindcss/vite": "^4.0.15",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.13.13",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "better-sqlite3": "^11.9.1",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-router-devtools": "^1.1.6",
    "stylelint": "^16.16.0",
    "stylelint-config-standard": "^37.0.0",
    "tailwindcss": "^4.0.15",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^3.114.2"
  },
  "packageManager": "pnpm@10.6.3+sha512.bb45e34d50a9a76e858a95837301bfb6bd6d35aea2c5d52094fa497a467c43f5c440103ce2511e9e0a2f89c3d6071baac3358fc68ac6fb75e2ceb3d2736065e6"
}
